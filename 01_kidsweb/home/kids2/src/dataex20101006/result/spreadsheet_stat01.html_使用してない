<script type="text/javascript"><!--
function $(tagId)
{
	return document.getElementById(tagId);
}

var __COLUMNS__ = 
_%__COLUMNS__%_
;

var __DATA__=
_%__DATA__%_
;

function displayData()
{
	$("result").innerHTML = "データ処理中です...";
}
function displayData1()
{
	$("result").innerHTML = parseJSON(__COLUMNS__, __DATA__);
}

// JSONのデータを解析して表示
function parseJSON(varColumns, varData)
{

	var columns = eval(varColumns);
	var data = eval(varData);

	var resultData = "<table>";
//alert(columns.length);
//alert(columns[0]["header"]);

	for(var j=0; j<columns.length; j++)
	{
		resultData += "<th>" + columns[j]["header"] + "&nbsp;</th>";
	}

	for(var i=0; i<data.length; i++)
	{
		resultData += "<tr>";
		for(var j=0; j<data[i].length; j++)
		{
			resultData += "<td>" + data[i][j] + "&nbsp;</td>";
		}
		resultData += "</tr>";
	}
	resultData += "</table>";
	return resultData;
}
// --></script>

</head>
<body onLoad="displayData1();">

   
