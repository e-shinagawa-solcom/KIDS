<bodyonload>
</bodyonload>
</config>

<!-- 左サイドバー ナビゲート -->
<div class="navigation-loadpoint">
    <script>
        $('.navigation-loadpoint').load('/inv/cmn/navigate/inv_navi.html');
    </script>
</div>

<!-- 入力フォーム -->
<div class="form-box--notabs-for-search">
    <!-- フォームヘッダ -->
    <div class="form-box__header">
        <p class="form-box__header__title">請求書検索</p>
        <span class="form-box__header__buttons">
            <img class="clear button" src="/img/type01/cmn/seg/sch_clear_off_ja_bt.gif">
            <img class="search button" src="/img/type01/cmn/seg/sch_search_off_ja_bt.gif">
        </span>
    </div>
    <!-- フォームコンテンツ -->
    <div class="form-box__contents--with-scroll">
        <form name="SearchMoldHistory" method="post">
            <input type="submit">
            <div class="option-box">
                <div class="regist-line" _%AdminSet_visibility%_>
                    <span class="regist-label">
                        <span class="regist-label--text-only">管理者モード</span>
                        <input name="Option_admin" type="checkbox">
                    </span>
                </div>
            </div>
            <div class="regist-line">
                <img class="toggle-search button" src="/img/type01/cmn/seg/off_bt.gif" alt="toggle search">
            </div>

            <div class="regist-line">
                <img src="/img/type01/cmn/seg/search_column_ja.gif" alt="view search">
            </div>

            <div class="regist-line">
                <span class="search-label--checkbox">
                    <input name="IsSearch_lngCustomerCompanyCode" class="is-search" type="checkbox">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">顧客</span>
                    <input class="TxtStyle05L" name="lngCustomerCompanyCode" type="text" iscustomer="yes" alt="CustomerCompany">
                    <input class="disTxt46L" name="strCustomerCompanyName" type="text" readonly>
                    <img class="msw-button button" invokeMSWName="msw-customer" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw">
                </span>
            </div>
            <div class="regist-line">
                <span class="search-label--checkbox">
                    <input name="IsSearch_strInvoiceCode" class="is-search" type="checkbox">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">請求書NO.</span>
                    <input class="TxtStyle06L" name="strInvoiceCode" type="text" onfocus="chColorOn(this);" onblur="chColorOff(this);">
                </span>
            </div>
            <div class="regist-line">
                <span class="search-label--checkbox">
                    <input name="IsSearch_dtmInvoiceDate" class="is-search" type="checkbox">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">請求日</span>
                    FROM <input class="TxtStyle05L" name="From_dtmInvoiceDate" type="text" value="" onfocus="chColorOn(this);" onblur="chColorOff(this);fncCheckDate(this);fncCopyValue( this , document.all.To_dtmDeliveryDate );" tabindex="" maxlength="10">
                    TO <input class="TxtStyle05L" name="To_dtmInvoiceDate" type="text" value="" onfocus="chColorOn(this);" onblur="chColorOff(this);fncCheckDate(this);" tabindex="" maxlength="10">
                </span>
            </div>
            <div class="regist-line">
                <span class="search-label--checkbox">
                    <input name="IsSearch_lngTaxClassCode" class="is-search" type="checkbox">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">消費税区分</span>
                    <select name="lngTaxClassCode" class="Slt13">
                        <option value=""></option>
                        <OPTION value=1>非課税</OPTION>
                        <OPTION value=2>外税</OPTION>
                        <OPTION value=3>内税</OPTION>
                    </select>
                </span>
            </div>
            <div class="regist-line">
                <span class="search-label--checkbox">
                    <input name="IsSearch_lngInsertUserCode" class="is-search" type="checkbox">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">起票者</span>
                    <input class="TxtStyle05L" name="lngInsertUserCode"  type="text" alt="InsertUser" displayFlagLimit="0">
                    <input class="disTxt46L" name="strInsertUserName"  type="text" readonly>
                    <img class="msw-button button" invokeMSWName="msw-inputuser" displayFlagLimit="0" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw">
                </span>
            </div>
            <div class="regist-line">
                <span class="search-label--checkbox">
                    <input name="IsSearch_lngInputUserCode" class="is-search" type="checkbox">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">入力者</span>
                    <input class="TxtStyle05L" name="lngInputUserCode"  type="text" alt="InputUser" displayFlagLimit="0">
                    <input class="disTxt46L" name="strInputUserName"  type="text" readonly>
                    <img class="msw-button button" invokeMSWName="msw-inputuser" displayFlagLimit="0" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw">
                </span>
            </div>

            <div class="regist-line">
                <span class="search-label--checkbox">
                    <input name="IsSearch_dtmInsertDate" class="is-search" type="checkbox">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">入力日</span>
                    FROM <input class="TxtStyle05L" name="From_dtmInsertDate" type="text" value="" onfocus="chColorOn(this);" onblur="chColorOff(this);fncCheckDate(this);fncCopyValue( this , document.all.To_dtmInsertDate );" tabindex="" maxlength="10">
                    TO <input class="TxtStyle05L" name="To_dtmInsertDate" type="text" value="" onfocus="chColorOn(this);" onblur="chColorOff(this);fncCheckDate(this);" tabindex="" maxlength="10">
                </span>
            </div>

            <input type="hidden" name="ActionScriptName" value="/inv/result/index.php">
            <input type="hidden" name="strSessionID" value="_%strSessionID%_">
        </form>
    </div>
    <div class="msw-loadpoints">
        <!-- MSW 顧客マスタ検索 -->
        <iframe class="msw-customer" src="/search/cmn/msw/customer/mswcustomer.html" frameborder="0">
        </iframe>
        <!-- MSW 入力者マスタ検索 -->
        <iframe class="msw-inputuser" src="/search/cmn/msw/inputuser/mswinputuser.html" frameborder="0">
        </iframe>
    </div>
</div>
