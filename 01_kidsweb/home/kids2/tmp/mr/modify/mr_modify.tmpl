<bodyonload>
</bodyonload>
</config>

<!-- 入力フォーム -->
<div class="form-box">
	<!-- フォームヘッダ -->
	<div class="form-box__header">
		<p class="form-box__header__title">金型帳票修正 (対象 : _%MoldReportId%_-_%Revision%_)</p>
	</div>
	<!-- フォームコンテンツ -->
	<div class="form-box__contents">
		<form name="RegistMoldReport" action="javascript:void(0);" method="post">
            <input type="submit">
			<div class="regist-tab-header">
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__text">製品コード</span>
						<input class="TxtStyle05L" name="ProductCode" value="_%Header_ProductCode%_" readonly>
						<input class="disTxt46L" name="ProductName" value="_%Header_ProductName%_" readonly>
					</span>
					<span class="regist-label">
						<span class="regist-label__text">顧客品番</span>
						<input class="TxtStyle05L" name="GoodsCode" value="_%Header_GoodsCode%_"  readonly>
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">帳票区分</span>
						<select name="ReportCategory" class="Slt13" init-value="_%ReportCategory%_">
							<option value="">−</option>
							<option value="10">移動版</option>
							<option value="20">返却版</option>
							<option value="30">廃棄版</option>
						</select>
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">依頼日</span>
						<input name="RequestDate" value="_%RequestDate%_" class="TxtStyle05L">
					</span>
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">依頼区分</span>
						<select name="RequestCategory" class="Slt25" init-value="_%RequestCategory%_">
							<option value="">−</option>
							<option value="10">KUWAGATA JAPAN REQUEST</option>
							<option value="20">KUWAGATA H.K. REQUEST</option>
						</select>
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">希望日</span>
						<input name="ActionRequestDate" value="_%ActionRequestDate%_" class="TxtStyle05L">
					</span>
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">移動方法</span>
						<select name="TransferMethod" class="Slt47" init-value="_%TransferMethod%_">
							<option value="">−</option>
							<option value="10">DELIVER TO RECEIVER(移動先に届ける)</option>
							<option value="20">PICK-UP BY RECEIVER(移動先が引き取りに来る)</option>
							<option value="30">VIA HONG KONG(香港経由)</option>
						</select>
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">指示区分</span>
						<select name="InstructionCategory" class="Slt13" init-value="_%InstructionCategory%_">
							<option value="">−</option>
							<option value="10">顧客指示</option>
							<option value="20">KWG指示</option>
						</select>
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">事業部(顧客)</span>
						<input class="TxtStyle05L" name="CustomerCode" value="_%CustomerCode%_">
						<input class="disTxt46L" name="CustomerName" readonly>
						<img class="msw-button button" invokeMSWName="msw-customer" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw">
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">KWG担当部署</span>
						<input class="TxtStyle05L" name="KuwagataGroupCode" value="_%KuwagataGroupCode%_">
						<input class="disTxt46L" name="KuwagataGroupName" readonly>
						<img class="msw-button button" invokeMSWName="msw-group" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw">
					</span>
				</div>
				<div class="regist-line--remora">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">KWG担当者</span>
						<input class="TxtStyle05L" name="KuwagataUserCode" value="_%KuwagataUserCode%_">
						<input class="disTxt46L" name="KuwagataUserName" readonly>
						<img class="msw-button button" invokeMSWName="msw-user" msw-before="pickupGroupCode" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw">
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">生産後の処理</span>
						<select name="FinalKeep" class="Slt39" init-value="_%FinalKeep%_">
                            <option value="">−</option>
                            <option value="10">KEEP BY RECEIVER(移動先工場で保管する)</option>
                            <option value="20">RETURN TO ORIGINAL(保管工場に返却する)</option>
                        </select>
					</span>
					<span class="regist-label">
						<span class="regist-label__required-mark"></span>
						<span class="regist-label__text">返却予定日</span>
						<input class="TxtStyle05L" name="ReturnSchedule" value="_%ReturnSchedule%_">
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark"></span>
						<span class="regist-label__text">その他</span>
						<input class="note" name="Note" value="_%Note%_">
					</span>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark"></span>
						<span class="regist-label__text">欄外備考</span>
						<textarea name="MarginalNote" rows="10">_%MarginalNote%_</textarea>
					</span>
				</div>
			</div>
			<div class="regist-tab-detail">
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__text">製品コード</span>
						<input class="TxtStyle05L" name="ProductCode" readonly>
						<input class="disTxt46L" name="ProductName" readonly>
					</span>
					<span class="regist-label">
						<span class="regist-label__text">顧客品番</span>
						<input class="TxtStyle05L" name="GoodsCode" readonly>
					</span>
				</div>
				<div class="regist-line">
					<table class="mold-selection">
		                <tr>
		                    <th class="mold-selection__label">金型リスト</th>
		                    <th></th>
							<th class="mold-selection__label">選択中の金型リスト</th>
		                    <th></th>
		                </tr>
						<tr>
		                	<td>
		                    	<select class="mold-selection__list Slt60" size="10" multiple="multiple"></select>
		                	</td>
		                	<td class="mold-selection__backimage-add-del">
								<img class="list-add button" src="/img/type01/cg/del_off_bt.gif" alt="list add">
								<img class="list-sort button" src="/img/type01/mr/sort_off_bt.gif" alt="list sort">
								<img class="list-del button" src="/img/type01/cg/add_off_bt.gif" alt="list del">
							</td>
							<td>
								<select name="ChoosenMoldList" class="mold-selection__choosen-list Slt60" size="10" multiple="multiple"></select>
							</td>
							<td class="mold-selection__backimage-up-down">
								<img class="list-up button" src="/img/type01/cg/up_off_bt.gif" alt="list up">
								<img class="list-sort button" src="/img/type01/mr/sort_off_bt.gif" alt="list sort">
								<img class="list-down button" src="/img/type01/cg/down_off_bt.gif" alt="list down">
							</td>
						</tr>
		            </table>
				</div>
				<div class="regist-line">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">保管工場</span>
						<input class="TxtStyle05L" name="SourceFactory" value="_%SourceFactory%_" readonly>
						<input class="disTxt46L" name="SourceFactoryName" value="_%SourceFactoryName%_" readonly>
						<img class="msw-button button" invokeMSWName="msw-factory" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw">
					</span>
				</div>
				<div class="regist-line--remora">
					<span class="regist-label">
						<span class="regist-label__required-mark">◎</span>
						<span class="regist-label__text">移動先工場</span>
						<input class="TxtStyle05L" name="DestinationFactory" value="_%DestinationFactory%_">
						<input class="disTxt46L" name="DestinationFactoryName" value="_%DestinationFactoryName%_" readonly>
						<img class="msw-button button" invokeMSWName="msw-factory" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw">
					</span>
				</div>
					<table class="table-description">
                        <div class="regist-line">
						<thead>
							<tr>
								<th>項番</th>
								<th>金型NO</th>
								<th>金型説明</th>
							</tr>
						</thead>
						<tbody>
						</tbody>
						<tfoot>
						</tfoot>
					</table>
				</div>
				<div class="regist-line">
				</div>
			</div>
		</form>
	</div>
	<div class="msw-loadpoints">
		<!-- MSW 工場マスタ検索 -->
		<iframe class="msw-factory" src="/mold/cmn/msw/factory/mswfactory.html" frameborder="0">
		</iframe>
		<!-- MSW 顧客マスタ検索 -->
		<iframe class="msw-customer" src="/mold/cmn/msw/customer/mswcustomer.html" frameborder="0">
		</iframe>
		<!-- MSW 担当部署マスタ検索 -->
		<iframe class="msw-group" src="/mold/cmn/msw/group/mswgroup.html" frameborder="0">
		</iframe>
		<!-- MSW 担当者マスタ検索 -->
		<iframe class="msw-user" src="/mold/cmn/msw/user/mswuser.html" frameborder="0">
		</iframe>
	</div>
</div>

<!-- 選択済み金型情報(画面初期化) -->
<div id="init-mold-info">
</div>


<!-- フォーム ボタン -->
<div class="form-buttons">
    <img class="form-buttons__clear button" src="/img/type01/cmn/querybt/clear_off_ja_bt.gif" alt="form clear">
    <img class="form-buttons__regist button" src="/img/type01/cmn/querybt/reg_off_ja_bt.gif" alt="form regist">
</div>

<!-- 右サイドバー タブ -->
<div class="tabs--modify">
	<img class="tabs__header" src="/img/type01/cmn/seg/h_tab_off.gif" alt="tab header" disabled>
	<img class="tabs__detail" src="/img/type01/cmn/seg/d_tab_on.gif" alt="tab footer">
</div>
