	<!-- セレクトボックス操作 -->
	<script src="/mr/regist/js/bind-selectbox-action.js" defer></script>
    <!-- フォーム ボタン -->
	<script src="/mr/regist/js/form-buttons.js" defer></script>
	<!-- 要素フォーカス時の挙動 -->
