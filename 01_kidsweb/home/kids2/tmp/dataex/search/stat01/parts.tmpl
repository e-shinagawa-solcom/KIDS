<Script for="objDataSourceSetting" event="ondatasetcomplete">subLoadMasterSetting(this.recordset,0);
</Script>



<form name="PS" method="post" action="/dataex/result/stat.php">
<input type="hidden" name="ActionScriptName" value="">
<input type="hidden" name="strSessionID" value="_%strSessionID%_">
<input type="hidden" name="lngExportData" value="_%lngExportData%_">
<input type="hidden" name="lngActionCode" value="1">




<!-- フォーム -->
<div class="form-box__contents">
	<div class="regist-line">
		<span class="form-line">
			<span class="Column">受注・納期</span>
			<span class="VarText" id="VarsA01">
				<span class="VarLabel">FROM</span>
				<input style="ime-mode:disabled;" class="Txt10L" type="text" name="dtmAppropriationDateFrom" value="_%lcdatestart%_" maxlength="10" onfocus="chColorOn(this);" onblur="chColorOff(this);fncCheckDate( this );" tabindex="">
				<span class="VarLabel">TO</span>
				<input style="ime-mode:disabled;" class="Txt10L" type="text" name="dtmAppropriationDateTo" value="_%lcdateend%_" maxlength="10" onfocus="chColorOn(this);" onblur="chColorOff(this);fncCheckDate( this );" tabindex="">
			</span>
		</span>
	</div>
	<div class="regist-line">
		<span class="form-line">
			<span class="Column">部門</span>
			<span class="VarPull" id="VarsA02">
				<select class="Slt20" name="lngGroupCode" tabindex="">
					_%lngGroupCode%_
				</select>
			</span>
		</span>
	</div>
	<div class="regist-line">
		<span class="form-line">
			<span class="Column">売上区分</span>
			<span class="VarCheck" id="VarsA03">
				<input class="CheckBox14" type="checkbox" name="lngSalesClassCode[]" value="0">全て　<br />
				_%lngSalesClassCode%_
			</span>
		</span>
	</div>
	<div class="Spacer">
	</div>
	<div class="QuerySet-none">	
		<!-- プレビューボタン -->
		<span class="QueryBt" id="PreviewBt" onclick="document.PS.lngActionCode.value=1;fncDataFormPreview( window.PS , '/dataex/result/stat.html' ); return false;"></span>
		<!-- エクスポートボタン -->
		<span class="QueryBt" id="ExportBt"  onclick="document.PS.lngActionCode.value=2;PS.submit();"></span>
	</div>
</div>
</form>
