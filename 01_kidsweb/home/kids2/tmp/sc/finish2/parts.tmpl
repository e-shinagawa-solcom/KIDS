<style>
.contents{
    position : relative;
    top : 180px;
    left :200px;
    width: 400px;
}
.regist-result {
    border-collapse: collapse;
}
.regist-result,tr,td{
    font-weight: bold;
    line-height: 1.5;
    padding: 5px;
}
.message-header {
    width:  100%;
    font-size : 16px;
    text-align: center;
    font-weight: bold;
}
.item-header {
    border: 1px solid black;
    color: #ffffff;
    background-color: #7e888f;
    text-align: center;
}
.item-value {
    border: 1px solid black;
    background : #e8f0f1;
    text-align: center;
    height: 35px;
}
.button-bottom {
    border: 1px solid black;
    padding:10px;
}
.close-footer {
    width:  100%;
    padding:10px;
    text-align: center;
}

/* カラム幅定義 */
.columnSlipCode{
    width:120px;
}
.columnInsertDate{
    width:120px;
}
.columnDownload{
    width:auto;
}

</style>

