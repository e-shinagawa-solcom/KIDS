<body id="Backs" onload="OnloadBody();" oncontextmenu="return false;">
    <input type="hidden" name="strSessionID" value="_%strSessionID%_">

    <div class="contents">

        <div class="message-header"><p>_%strMessage%_</p></div>
        <table class="regist-result">
            <thead>
                <th class="item-header columnSlipCode">納品書No</th>
                <th class="item-header columnInsertDate">登録日時</th>
                <th class="item-header columnDownload">ダウンロード</th>
            </thead>
