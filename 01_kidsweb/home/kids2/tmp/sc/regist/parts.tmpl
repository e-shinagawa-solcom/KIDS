	var WinCnt = 0;

	// 承認ルートボタン押下処理
	function fncOpenWFRoot( obj )
	{
		// 「承認なし」の場合の判定 -> 「承認なし」の場合はボタン押下禁止
		for( i=0; i < obj.options.length; i++ )
		{
			if( obj.options[i].text == '承認なし' )
			{
				if( i == 0 ) return false;
			}
		}

		if( WinCnt == 0 )
		{
			WFRootFrame.style.visibility = 'visible';
			WFrootBt.innerHTML = darkgrayOpenBt3;
			WinCnt = 1;
		}
		else if( WinCnt == 1 )
		{
			WFRootFrame.style.visibility = 'hidden';
			WFrootBt.innerHTML = darkgrayOpenBt1;
			WinCnt = 0;
		}

		return false;
	}

	function fncRedirectOpen( obj )
	{
		if( WinCnt == 1 )
		{
			fncOpenWFRoot();
		}
	}





	// ターゲットコードチェック
	function fncCheckTargetCode( code )
	{
		var strCode  = '';
		var blnCheck = false;

		strCode = trim( code );

		blnCheck = ( strCode == '' ) ? false : true;

		return blnCheck;
	}


	// 顧客受注番号から引き当て
	function fncOrderSubmit( code )
	{
		var lngLangCode = lngLanguageCode;
		var strAlert    = ( lngLangCode == 1 ) ? '顧客受注番号を入力してください。': 'Input the customer order code.';
		var strCode     = trim( code );

		document.HSO.strCustomerReceiveCode.value = strCode;

		if( strCode != "" )
		{
			document.HSO.action               = "index.php";
			document.HSO.strGetDataMode.value = "customer";
			document.HSO.strProcMode.value    = "onchange";
			document.HSO.submit();
		}
		else
		{
			alert( strAlert );
		}
	}

	// 製品コードから引き当て
	function fncOrderSubmit2( code )
	{
		var lngLangCode = lngLanguageCode;
		var strAlert    = ( lngLangCode == 1 ) ? '製品コードを入力してください。': 'Input the products code.';
		var strCode     = trim( code );

		document.HSO.strProductCodeOpen.value = strCode;

		if( strCode != "" )
		{
			document.HSO.action               = "index.php";
			document.HSO.strGetDataMode.value = "product";
			document.HSO.strProcMode.value    = "onchange";
			document.HSO.submit();
		}
		else
		{
			alert( strAlert );
		}
	}

