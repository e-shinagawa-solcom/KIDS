<!-- ログ -->
<form name="LOG" action="log.php" method="GET">
	<input type="hidden" name="strSessionID" value="_%strSessionID%_">

	<span id="LogBt" onclick="LOG.submit();return false;"></span> <!-- window.LOG.submit() -->

</form>



<!-- 登録 -->
<form name="REGIST" action="action.php" method="post">
	<input type="hidden" name="strSessionID" value="_%strSessionID%_">

	<div id="MessageSet">
		<div class="regist-line">
			<span class="form-line">
				<span class="Column" id="Column0">タイトル</span>
				<span class="VarText" id="Vars0">
					<input class="Txt90L" type="text" name="strSystemInformationTitle" onfocus="chColorOn(this);" onblur="chColorOff(this);" tabindex="1" value="_%strSystemInformationTitle%_">
				</span>
			</span>
		</div>
		<div class="regist-line">
			<span class="form-line">
				<span class="ColumnArea" id="Column1">メッセージ</span>
				<span class="VarTextArea" id="Vars1">
					<textarea rows="3" name="strSystemInformationBody" wrap="hard" onfocus="chColorOn(this);" onblur="chColorOff(this);fncCheckString( this , 200 );" tabindex="2" maxlength="200">_%strSystemInformationBody%_</textarea>
				</span>
			</span>
		</div>

		<span id="RegistBt" onclick="REGIST.submit();return false;"></span> <!-- window.REGIST.submit(); -->
	</div>

</form>



