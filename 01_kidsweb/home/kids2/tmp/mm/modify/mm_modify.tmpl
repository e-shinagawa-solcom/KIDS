<bodyonload>
</bodyonload>
</config>

<!-- 入力フォーム -->
<div class="form-box--notabs-for-modify">
	<!-- フォームヘッダ -->
	<div class="form-box__header">
		<p class="form-box__header__title">金型履歴修正</p>
        <div class="form-box__header__buttons">
            <a href="#"><img class="form-buttons__regist button" src="/img/type01/cmn/querybt/reg_off_ja_bt.gif" alt="form regist"></a>
        </div>
	</div>
	<!-- フォームコンテンツ -->
	<div class="form-box__contents">
		<form name="RegistMoldHistory" action="javascript:void(0);" method="post">
            <input type="submit">
            <div class="regist-line">
                <span class="regist-label">
                    <span class="regist-label__text">製品コード</span>
                    <input class="disTxt05L" name="ProductCode" value="_%ProductCode%_" disabled>
                    <span>_</span>
                    <input class="disTxt01L" name="ReviseCode" value="_%ReviseCode%_" disabled>
                    <input class="disTxt46L" name="ProductName" value="_%ProductName%_" disabled>
                    <input type="hidden" name="ProductCode" value="_%ProductCode%_">
                    <input type="hidden" name="ReviseCode" value="_%ReviseCode%_">
                    <input type="hidden" name="ProductName" value="_%ProductName%_">
                </span>
            </div>
            <div class="regist-line">
                <span class="regist-label">
                    <span class="regist-label__text">金型NO</span>
                    <input class="disTxt07L" name="MoldNo" value="_%MoldNo%_" disabled>
                    <input type="hidden" name="MoldNo" value="_%MoldNo%_">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">履歴番号</span>
                    <input class="disTxt05L" name="HistoryNo" value="_%HistoryNo%_" disabled>
                    <input style="display: none;" class="TxtStyle05L" name="Version" value="_%Version%_" disabled>
                    <input type="hidden" name="Version" value="_%Version%_">
                    <input type="hidden" name="HistoryNo" value="_%HistoryNo%_">
                </span>
            </div>
            <div class="regist-line">
                <span class="regist-label">
                    <span class="regist-label__required-mark">◎</span>
                    <span class="regist-label__required-text">実施日</span>
                    <input name="ActionDate" class="TxtStyle05L" value="_%ActionDate%_">
                </span>
                <span class="regist-label">
                    <span class="regist-label__text">金型ステータス</span>
                    <select name="Status" class="Slt13" init-value="_%DummyStatus%_">
                        <option value="">−</option>
                        <option value="10">移動</option>
                        <option value="20">返却</option>
                        <option value="30">廃棄</option>
                    </select>
                </span>
            </div>
            <div class="regist-line">
                <span class="regist-label">
                    <span class="regist-label__required-mark">◎</span>
                    <span class="regist-label__required-text">保管工場</span>
                    <input class="TxtStyle05L" name="SourceFactory" value="_%SourceFactory%_">
                    <input class="disTxt46L" name="SourceFactoryName" value="_%SourceFactoryName%_" disabled>
                    <span><a href="#"><img class="msw-button button" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw" invokeMSWName="msw-factory"></a></span>
                    <input type="hidden" name="SourceFactoryName" value="_%SourceFactoryName%_">
                </span>
            </div>
            <div class="regist-line--remora">
                <span class="regist-label">
                    <span class="regist-label__required-mark">◎</span>
                    <span class="regist-label__required-text">移動先工場</span>
                    <input class="TxtStyle05L" name="DestinationFactory"  value="_%DestinationFactory%_">
                    <input class="disTxt46L" name="DestinationFactoryName" value="_%DestinationFactoryName%_" disabled>
                    <span><a href="#"><img class="msw-button button" src="/img/type01/msw/cmn/open_off_bt.gif" alt="msw" invokeMSWName="msw-factory"></a></span>
                    <input type="hidden" name="DestinationFactoryName" value="_%DestinationFactoryName%_">
                </span>
            </div>
            <div class="regist-line">
                <span class="regist-label">
                    <span class="regist-label__required-mark"></span>
                    <span class="regist-label__text">備考1</span>
                    <textarea name="Remark1" rows="4">_%Remark1%_</textarea>
                </span>
            </div>
            <div class="regist-line">
                <span class="regist-label">
                    <span class="regist-label__required-mark"></span>
                    <span class="regist-label__text">備考2</span>
                    <textarea name="Remark2" rows="4">_%Remark2%_</textarea>
                </span>
            </div>
            <div class="regist-line">
                <span class="regist-label">
                    <span class="regist-label__required-mark"></span>
                    <span class="regist-label__text">備考3</span>
                    <textarea name="Remark3" rows="4">_%Remark3%_</textarea>
                </span>
            </div>
            <div class="regist-line">
                <span class="regist-label">
                    <span class="regist-label__required-mark"></span>
                    <span class="regist-label__text">備考4</span>
                    <textarea name="Remark4" rows="4">_%Remark4%_</textarea>
                </span>
            </div>
			<input type="hidden" name="strSessionID" value="_%strSessionID%_">
		</form>
	</div>
	<div class="msw-loadpoints">
		<!-- MSW 工場マスタ検索 -->
		<iframe class="msw-factory" src="/mold/cmn/msw/factory/mswfactory.html" frameborder="0">
		</iframe>
	</div>
</div>
