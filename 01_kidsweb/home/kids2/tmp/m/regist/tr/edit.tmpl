<script src="/cmn/jquery/jquery-cookie-1.4.1.js"></script>
<script type="text/javascript" language="javascript" src="/layout/type01/cmn/images.js"></script>
<script type="text/javascript" language="javascript" src="/layout/type01/m/cmn/images.js"></script>
<script type="text/javascript" language="javascript" src="/layout/type01/cmn/initlayoutcommon.js"></script>
<script type="text/javascript" language="javascript" src="/cmn/functions.js"></script>
<script type="text/javascript" language="javascript" src="/cmn/masterlib.js"></script>
<script type="text/javascript" language="javascript" src="/m/cmn/functions.js"></script>
<link rel="stylesheet" type="text/css" media="screen" href="/m/cmn/styles.css">
<link rel="stylesheet" type="text/css" media="screen" href="/m/cmn/layout.css">
<link rel="stylesheet" type="text/css" media="screen" href="/m/list/c/cmn/layout.css">
