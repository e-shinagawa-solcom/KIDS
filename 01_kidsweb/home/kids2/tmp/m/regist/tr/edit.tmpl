<body id="Backs" onload="fncEditObjectOnload();">
<form name="PS" action="confirm.php" method="GET">
<table cellpadding="5" cellspacing="0" border="0" bgcolor="#6f8180" align="center">
	<tr class="SegColumn">
		<td id="ControlTitle"></td>
	</tr>
</table>
<p id="objColumn">
	<span id="Column0" class="ColumnSegs">通貨単位コード</span>
	<span id="Column1" class="ColumnSegs">換算レート</span>
	<span id="Column2" class="ColumnSegs">適用開始月</span>
	<span id="Column3" class="ColumnSegs">適用終了月</span>
</p>

<p id="objInput">
	<span class="InputSegs">
		<select id="Input0" name="lngmonetaryunitcode" _%lngmonetaryunitcode_disabled%_>
			_%lngMonetaryUnitName%_
		</select>
	</span>
	<span class="InputSegs">
		<input id="Input1" type="text" name="curconversionrate" value="_%curconversionrate%_" onfocus="chColorOn(this);" onblur="chColorOff(this);">
	</span>
	<span class="InputSegs">
		<input id="Input2" type="text" name="dtmapplystartdate" value="_%dtmapplystartdate%_" onfocus="chColorOn(this);" onblur="chColorOff(this);" _%dtmapplystartdate_disabled%_>
	</span>
	<span class="InputSegs">
		<input id="Input3" type="text" name="dtmapplyenddate" value="_%dtmapplyenddate%_" onfocus="chColorOn(this);" onblur="chColorOff(this);">
	</span>

</p>
<br>
<p id="objTable">
	<table id="monetaryrateTbl" border="1" align="center" top="27px">
		<theard>
			<tr class="SegColumn">
				<th valign="center" id="monetaryrateTbl_heard">USドル：US$</th>
			</tr>
			<tr class="SegColumn">
				<th valign="center" class="seg">換算レート</th>
				<th valign="center" class="seg">適用開始月</th>
				<th valign="center" class="seg">適用終了月</th>
			</tr>
		</theard>
		<tbody id="monetaryrateTbl_tbody">
			_%monetaryRateHtml%_
		</tbody>
	</table>
</p>

<br>
<p id="QueryTable">
	<table cellpadding="5" cellspacing="0" border="1" bgcolor="#f1f1f1" align="center">
		<tr>
			<td>
				<span id="objQuery"></span>
					<input type="hidden" name="strSessionID" value="_%strSessionID%_">
					<input type="hidden" name="lngActionCode" value="_%lngActionCode%_">
					<input type="hidden" name="strSubmit" value="true">
					_%HIDDEN%_

			</td>
		</tr>
	</table>
</p>


<br>


</form>

</body>
</html>